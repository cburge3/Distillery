$("button").click(function(e) {
    e.preventDefault();
    $.ajax({
        type: "POST",
        url: "/generator",
        data: {
            id: $(this).attr("id"), // < note use of 'this' here
            time: "right now"
        },
        <!--success: function(result) {-->
            <!--alert('ok');-->
        <!--},-->
        error: function(result) {
            alert('error');
        }
    });
});